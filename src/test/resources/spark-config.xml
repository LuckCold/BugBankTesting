<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>Relatório de Execução dos Testes</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Execução</reportName>

        <!-- timestamp format -->
        <timeStampFormat>dd/MM/yyyy, HH:mm:ss</timeStampFormat>

        <offlineMode>true</offlineMode>
        <!--Show the thumbnail in report-->
        <thumbnailForBase64>true</thumbnailForBase64>
        <testViewChartLocation>bottom</testViewChartLocation>
        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {

                });
            ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[

            ]]>
        </styles>
    </configuration>
</extentreports>
